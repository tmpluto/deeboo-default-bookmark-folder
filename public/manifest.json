{
	"name": "deeboo Â· Default Bookmark Folder",
	"description": "Chrome Bookmark Manager Extension",
	"version": "1.2.0",
	"permissions": ["bookmarks", "tabs", "storage"],
	"manifest_version": 3,
	"options_ui": {
		"page": "options.html",
		"open_in_tab": true
	},
	"action": {
		"default_popup": "index.html"
	},
	"icons": {
		"16": "icons-circle/icon16.png",
		"32": "icons-circle/icon32.png",
		"48": "icons-spline/icon48.png",
		"128": "icons-spline/icon128.png"
	},
	"background": {
		"service_worker": "background.js"
	},
	"commands": {
		"_execute_action": {
			"suggested_key": {
				"default": "Ctrl+E",
				"mac": "Command+E"
			},
			"description": "Activate the extension..."
		}
	},
	"homepage_url": "https://github.com/tmpluto/deeboo-default-bookmark-folder"
}
